{
  "name": "Staff",
  "plural": "staffs",
  "base": "BaseUser",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {},
  "properties": {
    "digitalSignature": {
      "type": "string"
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
