{
  "name": "Student",
  "plural": "students",
  "base": "BaseUser",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamps": {},
    "Uuid": {}
  },
  "properties": {
    "studentCode": {
      "type": "string",
      "required": true
    },
    "career": {
      "type": "string",
      "required": true
    },
    "careerGrade": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": false,
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "enrollments": {
      "type": "hasMany",
      "model": "Event",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      },
      "through": "Enrollment"
    },
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW",
      "property": "signin"
    }
  ],
  "methods": {}
}
